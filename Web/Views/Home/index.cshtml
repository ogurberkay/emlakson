<!-- START SECTION FEATURED PROPERTIES -->
<div class="featured-projects pd-top-60 pd-bottom-70">
    <div class="container">
        <div class="section-title">
            <h2 class="title">Son Eklenen İlanlar</h2>
        </div>
        <div class="featured-slider slider-control-top">
            @{
            int k = 0;
            int i = 0;
            while (((i==0) ||(i%4 !=0)) && (i < Model.Count/4))
            {
                i++;
            <div class="item">
                <div class="row">
                    @for (int j = 0; j < 4; j++, k++)
                    {
                        var item = Model[k];
                        <div class="col-lg-3 col-sm-6">
                        <div class="single-feature" style="height: 100%">
                            <div class="thumb" style="height: 50%">
                                <img src="@Url.Content($"~/{@item.ImageWebPath}")" alt="img" style="height:100%; object-fit: cover;">
                            </div>
                            <div class="details" style="height: 50%">

                                <p class="author"><i class="fa fa-user"></i> Tunca Emlak</p>
                                <h6 class="title">
                                    <a href="property-details.html">@item.Title</a>
                                </h6>
                                
                                <h6 class="price mr-1">
                                    <a href="property-details.html">@item.AdvertType.ToString()</a>
                                </h6>
                                <h6 class="price mb-0 pb-0">@item.Price₺</h6>
                                <br/>
                                <ul class="info-list mt-2 pt-0">
                                    @if(item.BedroomNumber>0){
                                    <li><i class="fa fa-bed"></i> @item.BedroomNumber Yatak Odası</li>
                                    }
                                    @if (item.BathroomNumber > 0)
                                    {
                                        <li><i class="fa fa-bath"></i> @item.BathroomNumber Banyo</li>
                                    }
                                    @if(item.Meters >0){
                                    <li><img src="~/Template2/assets/img/icons/7.png" alt="img"> @item.Meters m²</li>
                                    }
                                </ul>
                                <ul class="contact-list">
                                    <li>
                                        <a class="phone" href="#">
                                            <i class="fa fa-phone"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a class="message" href="#">
                                            <img src="~/Template2/assets/img/icons/8.png" alt="img">
                                        </a>
                                    </li>
                                    <li>
                                        <a class="btn btn-yellow" href="property-details.html">İlanı Görüntüle</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    };

                </div>
            </div>

            }
            }
          

        </div>
    </div>
</div>
<!-- END SECTION FEATURED PROPERTIES -->


